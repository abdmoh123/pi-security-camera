{
    "name": "pisec-dev",
    "build": { "dockerfile": "Dockerfile" },
    "workspaceMount": "source=${localWorkspaceFolder},target=/workspace,type=bind,consistency=cached,Z",
    "workspaceFolder": "/workspace",
    "customizations": {},
    "forwardPorts": [8000],
    "postCreateCommand": "uv python install && uv sync --dev"
}
